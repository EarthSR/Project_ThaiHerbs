            <asp:Label ID="Label2" runat="server" Text="|" />&nbsp;
            <asp:LinkButton ID="login" runat="server" Text="Login" 
                CssClass="hyperlike-common" OnClick="login_Click"></asp:LinkButton>
        </div>
        <table class="auto-style3">
            <tr>
                <td class="auto-style4">
                    <asp:Image ID="Image1" runat="server" ImageUrl="~/Image/Green_Leaf_Minimalist__Organic_Shop_Logo-removebg-preview.png"
