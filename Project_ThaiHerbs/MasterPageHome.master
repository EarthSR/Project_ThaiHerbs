                <td class="auto-style2">
                    <asp:TextBox ID="TextBox2" runat="server" CssClass="search-input" placeholder="Search..." />
                </td>
                <td>
                    <asp:Button ID="Button3" runat="server" Text="Search" CssClass="flexible-button" />
                    &nbsp;
                    <asp:Button ID="Button4" runat="server" Text="Cart" CssClass="flexible-button" />
                </td>
            </tr>

        </table>
        <div class="typeproduct">
