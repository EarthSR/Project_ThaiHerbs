            <asp:Label ID="Label1" runat="server" Text="|" />&nbsp;
            <asp:HyperLink ID="HyperLink1" runat="server" NavigateUrl="~/Login.aspx" Text="Support"
                CssClass="hyperlike-common" />&nbsp;
            <asp:Label ID="Label3" runat="server" Text="|" />&nbsp;
            <asp:LinkButton ID="register" runat="server" Text="Register" 
